@model IEnumerable<Garage.Domain.Entities.ParkingSpot>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Arrival)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vehicle)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Arrival)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vehicle.Brand)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ParkingSpotId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ParkingSpotId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ParkingSpotId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
